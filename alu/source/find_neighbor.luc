module find_neighbor (
    input c[16],  // clock
    output s[16]
  ) {

  always {
    case(c[8:0]){
      b100000000:s=16b110100000;
      b010000000:s=16b111010000;
      b001000000:s=16b011001000;
      b000100000:s=16b100110100;
      b000010000:s=16b010111010;
      b000001000:s=16b001011001;
      b000000100:s=16b000100110;
      b000000010:s=16b000010111;
      b000000001:s=16b000001011;
      default:s=c;
      }
  }
}
